<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="br.edu.infnet.walletapp">

<application
    android:allowBackup="false"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.WalletApp">
    <activity android:name=".ui.AddContaActivity" android:label="@string/adicionar_conta"/>
    <activity android:name=".ui.CadastroActivity" />
    <activity android:name=".ui.MainActivity" />
    <activity android:name=".ui.LoginActivity"/>
    <activity android:name=".ui.SplashScreenActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        <nav-graph android:value="@navigation/cadastro_navigation" />
        <nav-graph android:value="@navigation/extrato_navigation" />
    </activity>
</application>

</manifest>
